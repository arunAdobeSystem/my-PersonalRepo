<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="PaccalVet Component Dialog"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Form Configaration">
                <items jcr:primaryType="cq:WidgetCollection">
                    <redirect
                        jcr:primaryType="cq:Widget"
                        componentId="redirectField"
                        fieldDescription="Leave empty to redisplay the form after submission"
                        fieldLabel="Thank You Page"
                        name="./redirect"
                        xtype="pathfield"/>
                    <member
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Group to assign to registered User"
                        fieldLabel="Initial Group"
                        name="./memberOf"
                        xtype="textfield"/>
                    <home
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Page to display upon login success"
                        fieldLabel="Home"
                        name="./home"
                        xtype="pathfield"/>
                    <intermediatePath
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Relative path that defines the location where the new account is created."
                        fieldLabel="Path"
                        name="./intermediatePath"
                        xtype="pathfield"/>
                    <report
                        jcr:primaryType="cq:Widget"
                        url="/etc/importers/bulkeditor.html?rootPath=%2Fhome%2Fusers&amp;queryParams=resourceType%3AHome&amp;contentMode=false&amp;spc=true&amp;cv="
                        xtype="formreportbutton"/>
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldDescription="heading"
                        fieldLabel="Enter Title "
                        name="./heading"
                        xtype="textfield"/>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="cq:Panel"
                title="Left Hand Side Field Label Configuration ">
                <items jcr:primaryType="cq:WidgetCollection">
                    <_x0032_
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldDescription="Password"
                        fieldLabel="Password  Label"
                        name="./password"
                        xtype="textfield"/>
                    <_x0031_
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldDescription="Name "
                        fieldLabel="Name Label"
                        name="./name"
                        xtype="textfield"/>
                    <_x0033_
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldDescription="Address"
                        fieldLabel="Address  Label"
                        name="./address"
                        xtype="textfield"/>
                    <_x0034_
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        componentId="State"
                        fieldDescription="State  Label"
                        fieldLabel="State"
                        name="./state"
                        xtype="textfield"/>
                    <_x0035_
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Phone No"
                        fieldLabel="Phone No  Label"
                        name="./phoneno"
                        xtype="textfield"/>
                </items>
            </tab2>
            <tab3
                jcr:primaryType="cq:Panel"
                title="Right Hand Side Field Label Configuration ">
                <items jcr:primaryType="cq:WidgetCollection">
                    <_x0031_
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldDescription="Email Address"
                        fieldLabel="Email Address  Label"
                        name="./email"
                        xtype="textfield"/>
                    <_x0032_
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldDescription="Confirm Password"
                        fieldLabel="Confirm Password  Label"
                        name="./cpassword"
                        xtype="textfield"/>
                    <_x0033_
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldDescription="City"
                        fieldLabel="City  Label"
                        name="./city"
                        xtype="textfield"/>
                    <_x0034_
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldDescription="Zip Code"
                        fieldLabel="Zip Code  Label"
                        name="./zip"
                        xtype="textfield"/>
                    <_x0035_
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldDescription="Clinic"
                        fieldLabel="Clinic  Label"
                        name="./clinic"
                        xtype="textfield"/>
                </items>
            </tab3>
            <tab4
                jcr:primaryType="cq:Panel"
                title="Bottom Side Field and Button Label Configuration ">
                <items jcr:primaryType="cq:WidgetCollection">
                    <_x0031_
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldDescription="Updates "
                        fieldLabel="Updates Text"
                        name="./update"
                        xtype="textfield"/>
                    <_x0032_
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldDescription="Terms of Use"
                        fieldLabel="Terms Of Use Text"
                        name="./tou"
                        xtype="textfield"/>
                </items>
            </tab4>
        </items>
    </items>
</jcr:root>
