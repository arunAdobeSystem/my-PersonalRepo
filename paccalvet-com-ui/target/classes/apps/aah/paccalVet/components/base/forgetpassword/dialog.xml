<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="PaccalVet  Component Dialog"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Form Configaration">
                <items jcr:primaryType="cq:WidgetCollection">
                    <redirect
                        jcr:primaryType="cq:Widget"
                        componentId="redirectField"
                        fieldDescription="Leave empty to redisplay the form after submission"
                        fieldLabel="Thank You Page"
                        name="./redirect"
                        xtype="pathfield"/>
                    <resetpwpage
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Page to handle password change"
                        fieldLabel="Change Password Page"
                        name="./changePwdPage"
                        xtype="pathfield"/>
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldDescription="heading"
                        fieldLabel="Enter Title "
                        name="./heading"
                        xtype="textfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
