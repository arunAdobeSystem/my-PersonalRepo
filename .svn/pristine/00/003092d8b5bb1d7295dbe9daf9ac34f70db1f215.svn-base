<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Training Title Component Dialog"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Title Tab">
                <items jcr:primaryType="cq:WidgetCollection">
                    <text
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Enter Title "
                        name="./text"
                        xtype="textfield"/>
                    <link
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Enter Link"
                        name="./link"
                        xtype="pathfield"/>
                    <checkBobx
                        jcr:primaryType="cq:Widget"
                        defaultValue="true"
                        fieldLabel="Open in new Window"
                        inputValue="false"
                        name="./newWindow"
                        type="checkbox"
                        xtype="selection"/>
                    <_x0030_4
                        jcr:primaryType="cq:Widget"
                        defaultValue="true"
                        fieldLabel="Warn on leave?"
                        inputValue="false"
                        name="./wol"
                        type="checkbox"
                        xtype="selection"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
